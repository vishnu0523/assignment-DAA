def removeElement(nums, val):
    k = 0  
    for i in range(len(nums)):
        if nums[i] != val:
            nums[k] = nums[i]
            k += 1
    return k

# Example usage:
nums = [3, 2, 2, 3]
val = 3

k = removeElement(nums, val)
print(f"Array after removing {val}: {nums[:k]}, New length: {k}")
