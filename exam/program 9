def lengthOfLastWord(s):
   
    s = s.strip()
    
    words = s.split()
    
    if words:
        return len(words[-1])
    else:
        return 0

s1 = "Hello World"
print(lengthOfLastWord(s1))  

s2 = "   fly me   to   the moon  "
print(lengthOfLastWord(s2))  
s3 = "luffy is still joyboy"
print(lengthOfLastWord(s3))
