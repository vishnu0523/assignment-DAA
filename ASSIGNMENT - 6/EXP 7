import math

def minimum_subarrays(nums):
    n = len(nums)
    dp = [float('inf')] * n
    
    for i in range(n):
        gcd = 0 
        for j in range(i, -1, -1):
            gcd = math.gcd(gcd, nums[j]) 
            
            if gcd > 1:
                if j == 0:
                    dp[i] = 1 
                else:
                    dp[i] = min(dp[i], dp[j - 1] + 1)
    
    return dp[-1] if dp[-1] != float('inf') else -1
nums = [3,5]
print(minimum_subarrays(nums)) 
