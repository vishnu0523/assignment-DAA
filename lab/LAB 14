def linear_search(arr, target):
    for i in range(len(arr)):
        if arr[i] == target:
            return i  # O(n)
    return -1

# Recursive algorithm: Factorial
def factorial(n):
    if n == 0 or n == 1:
        return 1  # O(1)
    else:
        return n * factorial(n - 1)  # O(n)
print(linear_search([1, 2, 3, 4, 5], 4))  # O(n)
print(factorial(5))  # O(n)
